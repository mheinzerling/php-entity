{
  "name": "mheinzerling/entity",
  "description": "Simple ORM",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin Heinzerling",
      "email": "webmaster@mheinzerling.de"
    }
  ],
  "bin": [
    "bin/entity"
  ],
  "require": {
    "php": "^7.0.0",
    "symfony/console": "^3.1.0",
    "eloquent/enumeration": "^5.1.0",
    "mheinzerling/stringutils": "dev-master",
    "mheinzerling/fileutils": "dev-master",
    "mheinzerling/databaseutils": "dev-master",
    "mheinzerling/arrayutils": "@dev",
    "mheinzerling/jsonutils": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6.0",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "mheinzerling\\entity\\": [
        "src/"
      ]
    },
    "psr-0": {
      "mheinzerling": [
        "resources/tests/src/",
        "resources/tests/gensrc/"
      ]
    }
  }
}